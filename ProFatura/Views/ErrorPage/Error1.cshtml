
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>JustDo Admin</title>
    <link rel="stylesheet" href="~/tema/template/vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/tema/template/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/tema/template/css/vertical-layout-dark/style.css">
    <link rel="shortcut icon" href="~/tema/template/images/favicon.png" />
</head>

<body>
    <div class="container-scroller">
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="content-wrapper d-flex align-items-center text-center error-page bg-primary">
                <div class="row flex-grow">
                    <div class="col-lg-7 mx-auto text-white">
                        <div class="row align-items-center d-flex flex-row">
                            <div class="col-lg-6 text-lg-right pr-lg-4">
                                <h1 class="display-1 mb-0">404</h1>
                            </div>
                            <div class="col-lg-6 error-page-divider text-lg-left pl-lg-4">
                                <h2>ÜZGÜNÜM!</h2>
                                <h3 class="font-weight-light">Aradığın Sayfayı Bulamadık.</h3>
                            </div>
                        </div>
                        <div class="row mt-5">
                            <div class="col-12 text-center mt-xl-2">
                                <a class="text-white font-weight-medium" href="../Index">Ana Sayfaya Dön</a>
                            </div>
                        </div>
                        <div class="row mt-5">
                            <div class="col-12 mt-xl-2">
                                <p class="text-white font-weight-medium text-center">Copyright &copy; @DateTime.Now.Year All rights reserved.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/tema/template/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/tema/template/js/off-canvas.js"></script>
    <script src="~/tema/template/js/off-canvas.js"></script>
    <script src="~/tema/template/js/hoverable-collapse.js"></script>
    <script src="~/tema/template/js/template.js"></script>
    <script src="~/tema/template/js/settings.js"></script>
    <script src="~/tema/template/js/todolist.js"></script>
</body>

</html>
